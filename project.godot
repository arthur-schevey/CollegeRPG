; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="CollegeRPG"
run/main_scene="res://Levels/Campus.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://UI/HeartUIFull.png"

[autoload]

DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_width"
window/size/fullscreen=true

[editor]

import/use_multiple_threads=false

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg", "res://addons/magicavoxel_importer/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/todo_manager/plugin.cfg")

[gdnative]

singletons=["res://addons/godot-git-plugin/git_api.gdnlib"]

